{
  "name": "angular-note-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pm2 start ./bin/www --name angular-app-server",
    "reload": "pm2 reload angular-app-server",
    "watch": "pm2 start ./bin/www --name angular-app-server --watch",
    "stop": "pm2 stop all",
    "debug": "set DEBUG=angular-app-server:* & node ./bin/www"
  },
  "dependencies": {
    "bytes": "^3.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
