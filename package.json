    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "~1.9.0",
    "multer": "^1.3.1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "hashids": "^1.1.4",
    "log4js": "^3.0.4",
    "mysql": "^2.16.0",
    "pm2": "^3.0.3",
    "qn": "^1.3.0",
    "qs": "^6.5.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-parallel-uglify-plugin": "^1.1.0",
